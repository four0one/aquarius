<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context" xmlns:aquarius="http://www.rpcframework.com/schema/aquarius-client"
       xsi:schemaLocation="
	http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd http://www.rpcframework.com/schema/aquarius-client http://www.rpcframework.com/schema/aquarius-client.xsd">

    <context:annotation-config/>
    <context:component-scan base-package="com.rpcframework.client"/>
    <aquarius:client/>
    <aquarius:monitor address="127.0.0.1:8088"/>
</beans>
